{
    "name": "pythonphp/pythonphp",
    "type": "library",
    "description": "Ports of various python standard library functionality to PHP.",
    "keywords": ["python"],
    "homepage": "https://github.com/PythonPHP/PythonPHP",
    "license": "GPL3",
    "authors": [
        {
            "name": "Matthew Gamble"
        }
    ],
    "require": {
        "php": ">=7.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "6.1.3"
    },
    "autoload": {
        "files": [
            "src/functions_include.php"
        ],
        "psr-4": {
            "PythonPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PythonPHP\\Tests\\": "tests/"
        }
    }
}
